        </Label>
        <Label layoutX="267.0" layoutY="77.0" text="TeamName">
            <font>
                <Font name="Baskerville" size="13.0" />
            </font>
        </Label>
        <TextField fx:id="nameField" layoutX="220.0" layoutY="108.0" />
        <Label layoutX="285.0" layoutY="152.0" text="Rank">
            <font>
                <Font name="Baskerville" size="13.0" />
            </font>
        </Label>
        <TextField fx:id="rankField" layoutX="278.0" layoutY="186.0" prefHeight="26.0" prefWidth="56.0" />
        <Button fx:id="done" layoutX="284.0" layoutY="248.0" mnemonicParsing="false" onAction="#done" text="Save" />
